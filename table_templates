   <state name="on" data=" 00 20 00 20" />
   <state name="off" data=" 00 A0 00 A0" />
  </table>
  
<table type="Switch" name="Catalyst DTC Max Threshold Fix (P0420, P0430)" category="OBD-II" sizey="2">
